{
  "version": 2,
  "buildCommand": "cd ../.. && npx turbo run build --filter=@uai-demo/api-db",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

