        "framework": "vite"
      },
      {
        "name": "uai-server",
        "path": "apps/uai-server",
        "buildCommand": "npm run build",
        "outputDirectory": "dist",
        "devCommand": "npm run dev"
      },
      {
        "name": "api-db",
        "path": "apps/api-db",
        "buildCommand": "npm run build",
        "outputDirectory": "dist",
        "devCommand": "npm run dev"
      }
    ]
  }
}
